.main {
  width: 100%;
  flex-direction: column;
  background: #fff;
  padding: 0 30rpx;
  box-sizing: border-box;
}

page {
  background: #f3f3f3;
}

.list {
  flex-direction: row;
  justify-content: space-between;
  font-size: 28rpx;
  color: #555;
  border-bottom: 1rpx solid #eee;
  align-items: center;
  padding: 16rpx 0;
}

.list:last-child {
  border: none;
}

.list>view {
  flex-direction: row;
  align-items: center;
}

.list image {
  width: 17rpx;
  height: 20rpx;
  padding-left: 15rpx;
}

.list .photo {
  width: 56rpx;
  height: 56rpx;
  border-radius: 30rpx;
  padding: 0;
}

.pad_tb_30 {
  padding: 30rpx 0;
}
